<?xml version="1.0" encoding="utf-8" ?> 
<AmazonTestFramework>
	<RunSettings>
		<AUT>https://www.amazon.in/</AUT>
		<!-- Possible flag Chrome,Firefox, IE-->
		<Browser>Chrome</Browser>
		<ScreenshotPassPath>\AmazonTest\Screenshots\Pass\</ScreenshotPassPath>
		<ScreenshotFailPath>\AmazonTest\Screenshots\Fail\</ScreenshotFailPath>
		<!-- Possible flag Smoke, Regression, Unit  -->
		<TestType>SmokeTest</TestType>
		<!-- Possible Flag Y, N -->
		<IsLog>Y</IsLog>
		<LogPath>/AmazonTest/Logs/</LogPath>
		<BuildName>DemoBuild</BuildName>
		<!-- Possible Flag Y, N -->
		<IsReport>N</IsReport>
		<UserName>EAutoTeam</UserName>
		<!-- Possible flag Selenium, Specflow  -->
		<Technology>Selenium</Technology>
	</RunSettings>
	</AmazonTestFramework>